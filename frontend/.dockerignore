# Ignore node_modules - dependencies will be installed inside the container
node_modules/

# Ignore build directories (not needed unless you're using multi-stage builds)
build/
dist/

# Ignore environment files
.env
.env.local
.env.development
.env.test
.env.production

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Cache files
.cache/
.tmp/

# Test output
coverage/

# Local project files
.vscode/
.idea/
*.code-workspace

# Ignore macOS system files
.DS_Store

# Ignore Git configuration and metadata
.git/
.gitignore

# Ignore Docker files (these should not be copied into the container)
Dockerfile
docker-compose*.yml
